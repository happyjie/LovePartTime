<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CircleImageView">
        <attr name="border_width" format="dimension" />
        <attr name="border_color" format="color" />
    </declare-styleable>
    
    <declare-styleable name="MenuItemView">
        <attr name="next_arrow_enable" format="boolean"/>
        <attr name="is_show_icon" format="boolean"/>
        <attr name="is_show_next_arrow" format="boolean"/>
        <attr name="icon_width" format="dimension"/>
        <attr name="icon_height" format="dimension"/>
        <attr name="icon_res_id" format="reference"/>
        <attr name="text_size" format="dimension"/>
        <attr name="text_color" format="color"/>
        <attr name="menu_title" format="string"/>
        <attr name="background" format="reference"/>
    </declare-styleable>

    <declare-styleable name="KeyValueItemView">
        <attr name="key" format="string"/>
        <attr name="value" format="string"/>
        <attr name="keyTextSize" format="dimension"/>
        <attr name="keyTextColor" format="color"/>
        <attr name="valueTextSize" format="dimension"/>
        <attr name="valueTextColor" format="color"/>
    </declare-styleable>

    <declare-styleable name="ClearEditText">
        <attr name="drawableLeft" format="integer"/>
    </declare-styleable>

</resources>